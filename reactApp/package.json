{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate for project stracuture & tools for MERN stack",
  "main": "./server/template.js",
  "keywords": [
    "React"
  ],
  "scripts": {
    "transpile-server": "webpack --mode=development --config webpack.config.server.js",
    "transpile-client": "webpack --mode=development --config webpack.config.client.js",
    "run-server": "node ./dist/server.generated.js",
    "development": "nodemon",
    "build": "webpack --config webpack.config.client.production.js && webpack --mode=production --config webpack.config.server.js",
    "start": "NODE_ENV=production node ./dist/server.generated.js"
  },
  "engines": {
    "node": "13"
  },
  "author": "Safi",
  "license": "MIT",
  "dependencies": {
    "express": "^4.17.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "*",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "babel-loader": "*",
    "core-js": "^3.6.4",
    "nodemon": "^2.0.2",
    "webpack": "next",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "*"
  }
}
